      }},
      { "shadow04@progress_xp_text_label_template": {
          "color": [ 0, 0.2054, 0.1505 ],
          "offset": [ 0, 1 ],
          "layer": -2
      }}
  ]},
  "progress_xp_text_label_template": {
    "type": "label",
    "text": "#level_number",
    "color": [ 0.6156, 0.9019, 0.3058 ],
